{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["dom", "DOM.Iterable", "ESNext"],
    "module": "esnext",
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "jsx": "react"
  },
  "include": [
    "stories/src"
  ],
  "exclude": ["node_modules",
    "stories/src/**/*.stories.tsx"
  ]
}
